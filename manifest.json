{
  "$schema": "./manifest_schema.json",
  "manifest_version": 4,
  "type": "theme",
  "name": "主题模板",
  "slug": "theme_template",
  "description": "LiteLoaderQQNT 的主题模板",
  "version": "1.0.0",
  "icon": "./public/icon.png",
  "thumb": "./public/icon.svg",
  "authors": [
    {
      "name": "LiteLoaderQQNT",
      "link": "https://github.com/LiteLoaderQQNT"
    }
  ],
  "platform": ["win32", "linux", "darwin"],
  "injects": {
    "renderer": "./dist/renderer/index.js",
    "main": "./dist/main/index.cjs",
    "preload": "./dist/preload/index.cjs"
  },
  "repository": {
    "repo": "festoney8/Theme-Template",
    "branch": "main",
    "release": {
      "tag": "1.0.0",
      "file": "Theme-Template.zip"
    }
  }
}
